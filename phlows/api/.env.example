# Configurações do AWS Cognito para Phlow API

# User Pool ID
COGNITO_USER_POOL_ID=us-east-1_xxxxxxx

# Client ID público (para OAuth e registro)
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxx

# Client ID admin (para login com senha - precisa ser configurado no Cognito)
COGNITO_CLIENT_ID_ADMIN=seu_client_admin_id_aqui

# Domínio do Cognito
COGNITO_DOMAIN_APP_URI=https://xxxxxxxxxxx.auth.us-east-1.amazoncognito.com

# Domínio da API
COGNITO_DOMAIN_API_URI=https://xxxxxxxxxxx.auth.us-east-1.amazoncognito.com

# URI de callback para OAuth
CALLBACK_URI=http://localhost:3001/callback
CALLBACK_URI_ENCODED=http%3A//localhost%3A3001/callback

# Região da AWS
AWS_REGION=us-east-1

# URL do serviço de autorização
AUTHORIZATION_URI=http://localhost:3000

# URL do frontend
FRONTEND_URI=http://localhost:5173