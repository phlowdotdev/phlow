with:
  type: object
  required: true
  properties:
    path:
      type: string
      required: true
    method:
      type: string
      required: true
    response:
      type: object
      required: true

steps:
  - condition:
      assert: !phs main.path == setup.path && main.method == setup.method
    then:
      payload: !phs setup.response
